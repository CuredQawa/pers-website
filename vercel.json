{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.js" },
      { "src": "/(.*)", "dest": "/public/$1", "continue": true },
      { "src": "/.*", "dest": "/api/index.js" }
    ]
  }